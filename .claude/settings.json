{
  "model": "sonnet[1m]",
  "env": {
    "ENABLE_TOOL_SEARCH": "true",
    "ENABLE_EXPERIMENTAL_MCP_CLI": "false",
    "MCP_TIMEOUT": "30000"
  },
  "statusLine": {
    "type": "command",
    "command": "npx --yes ccstatusline@latest"
  },
  "enableAllProjectMcpServers": true,
  "enabledMcpjsonServers": [
    "memory",
    "playwright"
  ]
}
