{
  "mcpServers": {
    "memory": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-memory@latest"],
      "env": {},
      "description": "メモリサーバー - セッション間の情報保持"
    },
    "github": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-github@latest"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "YOUR_GITHUB_TOKEN_HERE"
      },
      "description": "GitHub統合 - リポジトリ管理、Issue追跡、PR作成"
    },
    "brave-search": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-brave-search@latest"],
      "env": {
        "BRAVE_API_KEY": "YOUR_BRAVE_API_KEY_HERE"
      },
      "description": "Brave検索 - Web検索、最新情報取得"
    },
    "context7": {
      "type": "http",
      "url": "https://api.context7.com/v1/mcp",
      "headers": {
        "Authorization": "Bearer YOUR_CONTEXT7_API_KEY_HERE"
      },
      "description": "Context7 - 拡張コンテキスト管理、長期記憶"
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-sequential-thinking@latest"],
      "env": {},
      "description": "Sequential Thinking - 順序立った思考、複雑な問題解決"
    },
    "sqlite": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@modelcontextprotocol/server-sqlite@latest"],
      "env": {},
      "description": "SQLiteデータベース - 開発・テスト用データストア"
    },
    "claude-mem": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@anthropic-ai/mcp-server-memory@latest"],
      "env": {},
      "description": "Claude Memory - 長期記憶管理、コンテキスト保持"
    }
  }
}
